{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^http://127.0.0.1"
    }
  ],
  "replacementPatterns": [],
  "httpHeaders": [
    {
      "urls": ["https://opengitops.dev"],
      "headers": {
        "Accept": "text/html",
        "User-Agent": "Mozilla/5.0 (compatible; markdown-link-check)"
      }
    }
  ],
  "timeout": "30s",
  "retryOn429": true,
  "retryCount": 5,
  "fallbackRetryDelay": "10s",
  "aliveStatusCodes": [200, 206, 301, 302, 403]
}
